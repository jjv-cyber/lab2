<?xml version="1.0" encoding="UTF-8"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro" name="lab">

    <xacro:include filename="rrbot_on_table_macro.urdf.xacro" />
    <xacro:include filename="materials.xacro" />
    <xacro:include filename="table_macro.xacro" />
    <xacro:include filename="rrbot_macro.xacro" />
    <link name="world" />

    <xacro:lab_table prefix="t1" parent="world">
        <origin xyz="2 0 0" rpy="0 0 0" />
