    <xacro:lab_table prefix="t2" parent="world">
        <origin xyz="0 2 0" rpy="0 0 0" />
    </xacro:lab_table>

    <xacro:lab_table prefix="t3" parent="world">
        <origin xyz="2 2 0" rpy="0 0 0" />
    </xacro:lab_table>

    <xacro:lab_table prefix="t4" parent="world">
        <origin xyz="0 0 0" rpy="0 0 0" />
    </xacro:lab_table>

    <xacro:lab_table prefix="t5" parent="world">
        <origin xyz="-2 2 0" rpy="0 0 0" />
    </xacro:lab_table>

    <xacro:lab_table prefix="t6" parent="world">
        <origin xyz="-2 0 0" rpy="0 0 0" />
    </xacro:lab_table>

    <xacro:lab_table prefix="t7" parent="world">
        <origin xyz="-2 -2 0" rpy="0 0 0" />
    </xacro:lab_table>

    <xacro:lab_table prefix="t8" parent="world">
        <origin xyz="0 -2 0" rpy="0 0 0" />
    </xacro:lab_table>

    <xacro:lab_table prefix="t9" parent="world">
        <origin xyz="2 -2 0" rpy="0 0 0" />
    </xacro:lab_table>
</robot>